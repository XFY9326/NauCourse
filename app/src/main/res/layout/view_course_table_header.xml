<?xml version="1.0" encoding="utf-8"?>
<tool.xfy9326.naucourse.ui.views.widgets.AdvancedLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/layout_courseTableHeader"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginEnd="@dimen/course_cell_padding_half"
    android:gravity="center_vertical"
    android:minHeight="@dimen/course_table_header_min_height">

    <FrameLayout
        android:layout_width="@dimen/course_table_course_time_row_size"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/tv_cellMonth"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="@dimen/course_cell_padding"
            android:gravity="center"
            android:textSize="12sp" />

    </FrameLayout>

</tool.xfy9326.naucourse.ui.views.widgets.AdvancedLinearLayout>